import sys

def factorize(n):
    factors = []
    for i in range(2, n + 1):
        while n % i == 0:
            factors.append(i)
            n //= i
        if n == 1:
            break
    return factors

if len(sys.argv) != 2:
    print("Usage: factors <file>")
    sys.exit(1)

input_file = sys.argv[1]

try:
    with open(input_file, 'r') as file:
        for line in file:
            n = int(line)
            factors = factorize(n)
            if len(factors) >= 2:
                p, q = factors[0], factors[1]
                print(f"{n}={p}*{q}")

except FileNotFoundError:
    print(f"Error: File '{input_file}' not found.")
    sys.exit(1)

except ValueError:
    print("Error: Invalid input. Make sure the file contains only natural numbers greater than 1.")
    sys.exit(1)

